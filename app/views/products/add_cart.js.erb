$('#emptyNoti').hide();
$("#product_action_<%= @product.id%>").html(
   "<%= escape_javascript(render(partial: 'product_action', locals: {product: @product})) %>"
  )
<%if @cart.quality == 1 %>

  $('#list_carts').append(
    "<%= escape_javascript(render(partial: 'item_cart', locals: {productC: @product})) %>"
  );
<%else%>
  $("#item_quality_<%= @product.id %>").html("<%=@cart.quality%>");
<%end%>
$('#total_price_cart').html("<%= number_to_currency(@total_price)%>")
